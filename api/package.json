{
  "name": "@administracion-ligas-deportivas/api",
  "version": "1.0.0",
  "description": "API para Administración de Ligas Deportivas",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "format": "prettier --write . --cache",
    "format:fix": "npm run format && npm run lint:fix",
    "lint": "eslint . --ext .js,.ts,.cjs,.mjs --ignore-path .gitignore --cache",
    "lint:fix": "npm run lint -- --fix src",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:run": "sequelize-cli db:seed --seed",
    "seed:undo": "sequelize-cli db:seed:undo --seed",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "contributors": [
