    "directory": "api"
  },
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "mariadb": "3.0.2",
    "nodemon": "2.0.20",
    "sequelize-cli": "6.5.2",
    "sequelize": "6.28.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint": "8.30.0",
    "prettier": "2.8.1"
  }
}
